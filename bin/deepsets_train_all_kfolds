nohup python -u deepsets_train --test_kfold 3 --gpu 3 --nbits 4 --outdir deepsinv_synthable_4bits_8const_ptetaphi_kfolds/kfold_3 > ./logs/deepsinv_synthable_4bits_8const_ptetaphi_kfold_3 &
nohup python -u deepsets_train --test_kfold 4 --gpu 0 --nbits 4 --outdir deepsinv_synthable_4bits_8const_ptetaphi_kfolds/kfold_4 > ./logs/deepsinv_synthable_4bits_8const_ptetaphi_kfold_4 &
